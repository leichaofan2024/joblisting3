<%= simple_form_for [:admin,@job] do |f| %>
  <%= f.input :title, input_html: {:class => "control-form"} %>
  <%= f.input :description, input_html: {class: "control-form"} %>
  <%= f.input :wage_upper_bound, input_html: {class:"control-form"} %>
  <%= f.input :wage_lower_bound, input_html: {class: "control-form"} %>
  <%= f.input :contact_email, input_html: {class: "control-form"} %>
  <%= f.input :is_hidden, input_html: { class: "control-form" } %>
  <%= f.submit "Submit",disable_with: "Submiting..." %>
<% end %>
